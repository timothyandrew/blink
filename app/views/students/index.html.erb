<h1>Students</h1>

<div class="student-actions pull-right">
  <%= link_to "New Student", new_student_path, class: "btn btn-primary" %>
</div>

<ul class="students-list list-group">
  <% @students.each do |student| %>
    <li class="students-list-item list-group-item"><%= link_to student.name, student_path(student) %></li>
  <% end %>
</ul>
